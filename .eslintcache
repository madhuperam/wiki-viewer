ore = this.wikiStore.bind(this);\n  }\n  wikiStore(){\n    var links = [];\n    console.log(typeof keys);\n    for(var i=0;i<keys.length;i++){\n      var link={\n        txt: gotData.pages[keys[i]].extract,\n        heading: gotData.pages[keys[i]].title,\n        pageId: gotData.pages[keys[i]].pageid\n      }\n      links.push(link);\n    }\n    this.setState(()=>({\n      link: links\n    }));\n  }\n\n\n\n  searchPage(){\n    if(!this.state.search){\n      document.getElementById(\"search\").classList.add(\"hidden\");\n      document.getElementById(\"search-bar\").classList.remove(\"hidden\");\n\n      document.getElementById(\"search-txt\").addEventListener(\"keyup\",(event)=>{\n        if(event.keyCode === 13 && event.target.value !== \"\"){\n          document.getElementById(\"App\").style.top=0;\n          document.getElementById(\"info\").classList.add(\"hidden\");\n          fetchJsonp(url+event.target.value)\n            .then(response => response.json())\n            .then(data =>{\n              gotData = data.query;\n              keys  = Object.keys(data.query.pages);\n              this.wikiStore();\n            });\n        }\n      });\n    }else{\n      document.getElementById(\"search\").classList.remove(\"hidden\");\n      document.getElementById(\"search-bar\").classList.add(\"hidden\");\n      document.getElementById(\"info\").classList.remove(\"hidden\");\n      document.getElementById(\"App\").style.top=\"200px\";\n      gotData ={};\n      keys={};\n      this.wikiStore();\n    }\n\n    this.setState((state)=>({\n      search: !state.search\n    }));\n\n  }\n  render(){\n    return(\n      <div id=\"App\">\n        <a href=\"https://en.wikipedia.org/wiki/Special:Random\" target=\"_blank\"><p className=\"txt\">Click here for a random article</p></a>\n        <br />\n        <div id=\"search-bar\"className= \"hidden\">\n          <input type=\"search\" id=\"search-txt\" />\n          <button className=\"search-btn btn btn-default\" onClick={this.searchPage}><i className=\"fas fa-times\" /></button>\n        </div>\n        <button className=\"btn btn-default\" id=\"search\" onClick={this.searchPage}><i className=\"fas fa-search\" /></button>\n        <br />\n        <p className=\"txt\" id=\"info\">Click icon to search</p>\n        <Display links={this.state.link} />\n      </div>\n    );\n  }\n}\n\nclass Display extends React.Component{\n  constructor(props){\n    super(props);\n  }\n\n  render(){\n    var lists=[];\n    var data = this.props.links;\n    for(var i=0;i<data.length;i++){\n      var hrefUrl = postUrl+this.props.links[i].pageId;\n      lists.push(<a href={hrefUrl} key={this.props.links[i].pageId} target=\"_blank\">\n        <div className=\"wiki-div\">\n            <h3 className=\"\">{this.props.links[i].heading}</h3>\n            <p>{this.props.links[i].txt}</p>\n        </div>\n      </a>);\n    }\n    return(\n      <div id=\"display\">\n      {lists}\n      </div>\n    )\n  }\n}\n\nexport default App;\n",{"ruleId":"26","replacedBy":"27"},{"ruleId":"28","replacedBy":"29"},{"ruleId":"26","replacedBy":"30"},{"ruleId":"28","replacedBy":"31"},{"ruleId":"32","severity":1,"message":"33","line":1,"column":8,"nodeType":"34","messageId":"35","endLine":1,"endColumn":12},{"ruleId":"32","severity":1,"message":"36","line":4,"column":8,"nodeType":"34","messageId":"35","endLine":4,"endColumn":16},{"ruleId":"37","severity":1,"message":"38","line":76,"column":64,"nodeType":"39","endLine":76,"endColumn":79},{"ruleId":"40","severity":1,"message":"41","line":92,"column":3,"nodeType":"42","messageId":"43","endLine":94,"endColumn":4},{"ruleId":"37","severity":1,"message":"38","line":101,"column":69,"nodeType":"39","endLine":101,"endColumn":84},"no-native-reassign",["44"],"no-negated-in-lhs",["45"],["44"],["45"],"no-unused-vars","'logo' is defined but never used.","Identifier","unusedVar","'ReactDOM' is defined but never used.","react/jsx-no-target-blank","Using target=\"_blank\" without rel=\"noreferrer\" is a 